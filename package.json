{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "drizzle-kit migrate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "stripe:setup": "tsx --env-file=.env stripeSetup.ts",
    "stripe:listen": "stripe listen --forward-to localhost:3000/webhook/stripe",
    "dev:stripe": "concurrently -k -n NEXT,STRIPE -c auto \"npm run dev\" \"npm run stripe:listen\""
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.96.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "dotenv": "^16.6.1",
    "drizzle-kit": "^0.24.2",
    "drizzle-orm": "^0.33.0",
    "lucide-react": "^0.428.0",
    "next": "^16.1.6",
    "postgres": "^3.4.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "stripe": "^16.12.0",
    "supabase": "^2.76.9",
    "tailwind-merge": "^2.6.1",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/node": "^20.19.33",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3"
  }
